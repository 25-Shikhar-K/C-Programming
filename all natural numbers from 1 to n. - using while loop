#include<stdio.h>
int main()
{
    int num, i = 0;
    printf("Enter N th term or Last term = ");
    scanf("%d", &num);
    if(num >0)
    {
        printf("List of Natural Numbers from 1 to %d are \n", num);
        while(i <= num)
        {
            printf("%d ", i++);
        }
    }
    else
    {
        printf("Enter Valid number\n");
    }
return 0;
}
